<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.felip.engetech">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".po"
            android:label="@string/title_activity_po"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ScrollingActivity"
            android:label="@string/title_activity_scrolling"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".PessoaFisica"
            android:label="@string/title_activity_pessoa_fisica"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".PessoaFisica1" />
        <activity android:name=".PessoaJuridica1" />
        <activity android:name=".Foto" />
        <activity android:name=".TipoDeResidencia" />
        <activity
            android:name=".TipoEstrutura"
            android:label="@string/title_activity_tipo_estrutura"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Tipos"></activity>
    </application>

</manifest>